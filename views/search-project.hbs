

<div class="container-fluid">

 
  <label for="search">Wanna know which project are somewhere there?</label>
    
    <div class="d-flex container-fluid bg1">
      <div class="col-6">
        <form action="/search-project" method="POST">
           <input id="searchicon" class="btn-block" type="text" name="search" placeholder="Project name">
      </div>

      <div class="col-4">
        <button type="submit" class="btn btn-outline-dark btn-block">Search</button>
      </div>
      </form>
    </div>

    <div class="container-fluid bg">
      
          {{#each result}}
          <div class="project_dashboard d-flex justify-content-between">
            <p>{{this.projectName}}</p>
            {{!-- <div class="add btn">
             use this one or the other
            </div> --}}
            <div class="addtest btn btn-outline-dark">
            Add
            </div>
  
          </div>
          {{/each}}

            
          
    
    </div>
</div>

