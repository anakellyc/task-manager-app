
<div class="container d-flex justify-content-center">
  <div class="register">
    <h1>Create Project</h1>
  </div>

  <div class="register">
    <form action="/create-project" method="post">
        
      <label for="projectName">Project Name</label><br>
      <input class="btn-block" type="text" name="projectName" placeholder="project name">
    
      <label for="description">Description</label><br>
      <textarea class="btn-block" type="text" name="description" placeholder="description"></textarea>
    
      <label for="startDate">Start Date</label><br>
      <input class="btn-block" type="date" name="startDate">
    
      <label for="endDate">End Date</label><br>
      <input class="btn-block" type="date" name="endDate">

      <input class="btn btn-outline-dark btn-block" type="submit" value="Submit">
    </form>
  </div>
</div>

  <h2>Task List</h2>
  <button class="btn btn-outline-dark btn-block"><a href="/add-task">Add Task</a></button>

  <h2>Contributors</h2>
   <button class="btn btn-outline-dark btn-block"><a href="/search-people">Search Contributors</a></button>

   <input class="small-button" type="submit" value="submit">
  </form>
